{

  "apiReport": {
    "enabled": false,
    "reportFolder": "<projectFolder>/static/api"
  },

  "docModel": {
    "enabled": false
  },

  "dtsRollup": {
    "enabled": true
  },

  "tsdocMetadata": {
    "enabled": false
  },

  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },

    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": true
      },

      "ae-missing-release-tag": {
        "logLevel": "none"
      }
    },

    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      },

      "tsdoc-undefined-tag": {
        "logLevel": "none"
      }
    }
  }
}
